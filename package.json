{
  "name": "react-highstock",
  "version": "1.0.5",
  "description": "React wrapper for highstock",
  "main": "index.js",
  "scripts": {
    "test": "echo \"No tests\"",
    "clean": "rimraf lib",
    "lint": "eslint src test",
    "build:umd": "webpack src/index.js dist/react-highstock.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/index.js dist/react-highstock.min.js --config webpack.config.production.js",
    "build": "npm run build:umd && npm run build:umd:min",
    "prepublish": "npm run test && npm run build"
  },
  "author": "Ken Fehling",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kenfehling/react-highstock"
  },
  "bugs": "https://github.com/kenfehling/react-highstock/issues",
  "keywords": [
    "chart",
    "react",
    "highcharts",
    "highstock",
    "graph"
  ],
  "dependencies": {
    "highstock-browserify": "^1.0.2"
  },
  "devDependencies": {
    "babel": "^5.6.4",
    "babel-core": "^5.6.4",
    "babel-loader": "^5.3.2",
    "webpack": "^1.7.2"
  },
  "peerDependencies": {
    "react": "^0.14.0"
  }
}
